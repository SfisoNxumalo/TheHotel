            "nullable": true
          },
          "roomId": {
            "type": "string",
            "format": "uuid"
          },
          "checkInDate": {
            "type": "string",
            "format": "date-time"
          },
          "checkOutDate": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "user": {
            "$ref": "#/components/schemas/UserEntity"
          },
          "room": {
            "$ref": "#/components/schemas/RoomEntity"
          },
          "device": {
            "$ref": "#/components/schemas/DeviceEntity"
          },
          "orders": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RoomServiceOrderEntity"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DeviceEntity": {
        "required": [
          "deviceUUID"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          },
          "updatedDate": {
            "type": "string",
            "format": "date-time"
          },
          "isDeleted": {
            "type": "boolean"
          },
          "deviceUUID": {
            "minLength": 1,
            "type": "string"
          },
          "roomId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "room": {
            "$ref": "#/components/schemas/RoomEntity"
          }
        },
        "additionalProperties": false
      },
      "MenuItemDTO": {
        "required": [
          "available",
          "description",
          "id",
          "image",
          "itemName",
          "price"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "itemName": {
            "type": "string",
            "nullable": true
          },
          "image": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "available": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "MessageEntity": {
        "required": [
          "messageText",
          "senderId",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          },
          "updatedDate": {
            "type": "string",
            "format": "date-time"
          },
          "isDeleted": {
            "type": "boolean"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "staffId": {
            "type": "string",
            "format": "uuid"
          },
          "senderId": {
            "type": "string",
            "format": "uuid"
          },
          "messageText": {
            "maxLength": 1000,
            "minLength": 0,
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/UserEntity"
          },
          "staff": {
            "$ref": "#/components/schemas/StaffEntity"
          }
        },
        "additionalProperties": false
      },
      "OrderItemDTO": {
        "required": [
          "id",
          "itemName",
          "note",
          "price",
          "quantity"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "itemName": {
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "note": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "OrderRoomServiceDTO": {
        "required": [
          "createdAt",
          "items",
          "note",
          "orderId",
          "status",
          "userContact",
          "userId",
          "userName"
        ],
        "type": "object",
        "properties": {
          "orderId": {
            "type": "string",
            "format": "uuid"
